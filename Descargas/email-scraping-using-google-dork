function getSelection(node2){const nodeValue=(node2=node2||{}).value||"",selection={start:nodeValue.length,end:nodeValue.length,direction:"forward"};try{(node2.selectionStart||0===node2.selectionStart)&&(selection.start=node2.selectionStart,selection.end=node2.selectionEnd?node2.selectionEnd:0,selection.direction=node2.selectionDirection?node2.selectionDirection:"none")}catch{}return selection}

